<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<!-- Do use PrettyFaces ONLY for navigation -->

	<navigation-rule>
		<from-view-id>*</from-view-id>
		
		<navigation-case>
			<from-outcome>userAccountRequestCreateByNotConnectedUserView</from-outcome>
			<to-view-id>/public/user/account/request/create.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>userAccountRequestFindByNotConnectedUserView</from-outcome>
			<to-view-id>/public/user/account/request/find.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>userAccountRequestReadByNotConnectedUserView</from-outcome>
			<to-view-id>/public/user/account/request/read.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>

</faces-config>
